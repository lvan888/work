	"item": [
		{
			"name": "Storm-001 GET请求-- 获取用户信息 成功",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "336a835b-6b53-438c-8787-452da5e5c6a3",
						"exec": [
							"pm.test(\"HTTP的协议状态码为 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"验证业务状态码为200\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.code).to.eql(200);",
							"});",
							"",
							"pm.test(\"验证名字的值\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.name).to.eql(\"小明\");",
							"});",
							"",
							"pm.test(\"验证年龄的值\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.age).to.eql(18);",
							"});",
							"",
							"pm.test(\"验证id的值\", function () {",
							"    pm.expect(pm.response.text()).to.include(1);",
							"});",
							"",
							"pm.test(\"验证id的值\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.id).to.eql(\"1\");",
							"});",
							"",
							"pm.test(\"Body is correct\", function () {",
							"    pm.response.to.have.body(\"{\\\"code\\\": 200, \\\"id\\\": \\\"1\\\", \\\"name\\\": \\\"\\\\u5c0f\\\\u660e\\\", \\\"age\\\": 18}\");",
							"});",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://{{ip}}/getuser?userid=1",
